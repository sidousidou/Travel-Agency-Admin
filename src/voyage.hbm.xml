<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
		  "-//Hibernat/Hibernat Mapping DTD 3.0//EN"
		  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
		  
<hibernate-mapping package="modele">
<class name="voyage" table="voyage">

	<id name="codeV" column="codeV" type="integer"  >
	<generator class="assigned" />
	
	</id>

	<property name="dateDebutV" column="dateDebutV" type="date" ></property>
	<property name="dateFinV" column="dateFinV" type="date"></property>
	<property name="idUserVoy" column="idUserVoy" type="integer"></property>
	<property name="codeCircuitV" column="codeCircuitV" type="integer"></property>
	<property name="idVilleV" column="idVilleV" type="integer"></property>
	
	
</class>


</hibernate-mapping>

		  