<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
		  "-//Hibernat/Hibernat Mapping DTD 3.0//EN"
		  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
		  
<hibernate-mapping package="modele">
<class name="hotel" table="hotel">

	<id name="idH" column="idH" type="integer">
	<generator class="native" />
	</id>

	<property name="adresseH" column="adresseH" type="string"></property>
	<property name="nbrChambreH" column="nbrChambreH" type="integer"></property>
	<property name="numTeleH" column="numTeleH" type="string"></property>
	<property name="classementH" column="classementH" type="string"></property>
	<property name="siteWebH" column="siteWebH" type="string"></property>
	
	
</class>


</hibernate-mapping>

		  