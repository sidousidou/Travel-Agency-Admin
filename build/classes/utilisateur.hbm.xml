<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
		  "-//Hibernat/Hibernat Mapping DTD 3.0//EN"
		  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
		  
<hibernate-mapping package="modele">
<class name="utilisateur" table="utilisateur">

	<id name="idU" column="idU" type="integer">
	<generator class="native" />
	</id>

	<property name="typeU" column="typeU" type="string"></property>
	<property name="nomU" column="nomU" type="string"></property>
	<property name="emailU" column="emailU" type="string"></property>
	<property name="motDePasseU" column="motDePasseU" type="string"></property>
	
	
</class>


</hibernate-mapping>

		  